{
	"name": "cambermast-website",
	"build": {
		"dockerfile": "../Dockerfile",
		"context": ".."
	},
	"workspaceFolder": "/workspaces/cambermast-website-new",
	"runArgs": ["-p", "5173:5173"],
	"forwardPorts": [5173],
	"overrideCommand": false,
	"portsAttributes": {
		"5173": {
			"label": "Vite dev server",
			"onAutoForward": "notify"
		}
	},
	"customizations": {
		"vscode": {
			"settings": {
				"task.allowAutomaticTasks": "on"
			},
			"extensions": [
				"openai.chatgpt",
				"svelte.svelte-vscode"
			]
		}
	}
}
